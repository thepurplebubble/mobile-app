{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "/**/*.ts",
    "/**/*.tsx",
    "/**/*.d.ts",
    "/**/*.js",
    "/**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(tabs)/index.js", "app/(tabs)/explore.jsx"  ]
}
